{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#F1FA8C",
          "properties": {
            "style": "full"
          },
          "style": "diamond",
          "template": "<b>{{ .Path }}</b> ",
          "type": "path"
        },
        {
          "foreground": "#43CCEA",
          "properties": {
	    "branch_icon": "",
	    "fetch_upstream_icon": true	
	  },
          "style": "diamond",
          "template": "<b>{{ .UpstreamIcon }} {{ .HEAD }}</b>",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#FFFFFF",
          "style": "plain",
          "template": ">",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}