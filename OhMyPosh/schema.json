{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#F1FA8C",
          "properties": {
            "style": "full"
          },
          "style": "diamond",
          "template": "\ue5ff <b>{{ .Path }}</b> ",
          "type": "path"
        },
        {
          "foreground": "#43CCEA",
          "properties": {
	    "branch_icon": "",
	    "fetch_upstream_icon": true,
	    "fetch_status": true	
	  },
          "style": "diamond",
          "template": "{{ .UpstreamIcon }} {{ .HEAD }}{{ if .Working.Changed }} \uF044  {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} \uF046  {{ .Staging.String }}{{ end }}",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#FFFFFF",
          "style": "plain",
          "template": ">",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}